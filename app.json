{
  "expo": {
    "name": "Telugu daily",
    "slug": "telugu-daily-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "YOUR_ANDROID_APP_ID_HERE",
          "iosAppId": "YOUR_IOS_APP_ID_HERE"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
